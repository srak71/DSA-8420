# random data
x <- runif(50, min = 0, max = 10)
y <- runif(50, min = 0, max = 10)
# scatter plot
plot(x,
y,
main = "Random Scatter Plot",
xlab = "Random X",
ylab = "Random Y",
col = rainbow(50),
pch = 19)
